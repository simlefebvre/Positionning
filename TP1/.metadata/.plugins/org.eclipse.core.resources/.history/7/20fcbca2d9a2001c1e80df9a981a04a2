package fr.utbm.positionning.tp1.MainClass;

public class Emmetor {
	
	private Coordinate coo;
	private float dist;
	
	public Emmetor(Coordinate coo,float dist) {
		this.coo = coo;
		this.dist = dist;
	}

	public Coordinate getCoo() {
		return coo;
	}

	public float getDist() {
		return dist;
	}
	
	

}
